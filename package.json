{
    "name": "vscode-mal",
    "author": {
        "name": "Oskar Joelsson",
        "url": "https://github.com/orrpan/vscode-MAL"
    },
    "displayName": "vscode-mal",
    "description": "Meta Attack Language for vscode",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "mal",
                "aliases": [
                    "Meta Attack Language",
                    "mal"
                ],
                "extensions": [
                    ".mal"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "mal",
                "scopeName": "source.mal",
                "path": "./syntaxes/mal.tmLanguage.json"
            }
        ]
    }
}